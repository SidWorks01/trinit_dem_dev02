{
    "manifest_version": 3,
    "name": "DEM CO2 Footprint",
    "description": "An Extension that detects the Carbon Footprint of Websites",
    "version": "1.0",
    "action": {
		"default_popup": "index.html",
		"default_icon": {
			"48": "./images/icon-48.png"
		}
    }, 
	"permissions": ["unlimitedStorage", "activeTab", "scripting"]
}
